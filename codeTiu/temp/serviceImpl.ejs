package <%= package %>; 

import java.util.List;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import <%= importEntity %>; 
import <%= importDao %>; 
import <%= importService %>;
/**
 *  
 * Copyright (c) 2017 by [ <%= config.company %>] 
 * @Description:  <%=table.comment %>  
 * @author:       <%=config.author %>
 * @version:      <%=config.version %>  
 * @Date:       <%= Date.today().toFormat("YYYY-MM-DD HH:MM:SS")%>
 */
 
@Service
public class <%=table.name %><%= config.package.serviceImpl.name%> implements <%= table.name %><%= config.package.service.name%>  {
	@Autowired
	private <%=table.name %><%=config.package.dao.name.toLowerCase()%> <%=table.paramName %><%=config.package.dao.name%>;
	/**
	 * 根据主键查询
	 * @param primekey 平台主键
	 * @return
	 */
    public <%=table.name %> findByPrimaryKey(String primekey){
      return <%=table.paramName %><%=config.package.dao.name%>.findByPrimaryKey(primekey);
    }
	 /**
	 * 根据条件查询
	 * @param <%= table.paramName %> 实体条件
	 * @return
	 */
    public List<<%= table.name %>> findByCondition(<%= table.name %> <%= table.paramName %>){
      return <%=table.paramName %><%=config.package.dao.name%>.findByCondition(<%= table.paramName %>);
  }
	/**
	 * 更新
	 * @param <%= table.paramName %>  
	 * @return
	 */ 
	public int update(<%= table.name %> <%= table.paramName %>){
      return <%=table.paramName %><%=config.package.dao.name%>.update(<%= table.paramName %>);
    }
	/**
	 * 根据主键删除
	 * @param primekey  
	 * @return
	 */ 
	public int deleteByPrimaryKey(String primekey){
      return <%=table.paramName %><%=config.package.dao.name%>.deleteByPrimaryKey(primekey);
    }
	/**
	 * 插入
	 * @param <%= table.paramName %>  
	 * @return
	 */ 
	public int insert(<%= table.name %> <%= table.paramName %>){
      return <%=table.paramName %><%=config.package.dao.name%>.insert(<%= table.paramName %>);
    }
	/**
	 * 批量插入
	 * @param <%= table.paramName %>s
	 * @return
	 */ 
	public int insertBatch(List<<%= table.name %>>  <%= table.paramName %>s){
      return <%=table.paramName %><%=config.package.dao.name%>.insertBatch(<%= table.paramName %>s);
    }
}
